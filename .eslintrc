{
  "extends": ["airbnb"],
  "plugins": ["react", "react-hooks"],
  "rules": {
    "indent": 0,
    "no-tabs": 0,
    "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"] }],
    "jsx-a11y/media-has-caption": [0, {
        "audio": [ "Audio" ],
        "video": [ "Video" ],
        "track": [ "Track" ],
      }],
      "jsx-a11y/anchor-is-valid": [ "error", {
        "components": [ "Link" ],
        "specialLink": [ "to", ],
      }],
      "react-hooks/rules-of-hooks": "error"
  },
  "env": {
    "browser": true,
  },
  "parser": "babel-eslint",
}
